#include <iostream>
int main() {
  float nota1;
  float nota2;
  float nota3;
  float nota4;
  float media;
  
  std::cout<< "digite suas notas\n"; 
  std::cin>> nota1>> nota2>> nota3>> nota4;
  media = (nota1 +nota2 +nota3 +nota4);
media = media/4;
  std::cout<< "a media do aluno e\n" <<media;

  return 0; }



  #include <iostream>
int x, y;

std::string soma(int num1, int num2) {
std::string resultado= std::to_string(num1+num2);
return resultado;
}

int main() {

  std::cout << "Write your number";
  std::cin >> x;
  std::cout << "Write your second number";
  std::cin>> y;
  std::cout<< soma(x, y);

  return 0;};


